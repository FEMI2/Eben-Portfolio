<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 180 180" width="180" height="180">
  <defs>
    <linearGradient id="bgGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#3b82f6;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#1d4ed8;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="iconGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#ffffff;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#e5e7eb;stop-opacity:1" />
    </linearGradient>
    <filter id="shadow" x="-20%" y="-20%" width="140%" height="140%">
      <feDropShadow dx="0" dy="4" stdDeviation="4" flood-color="#000000" flood-opacity="0.3"/>
    </filter>
  </defs>
  
  <!-- Background with rounded corners for iOS -->
  <rect x="0" y="0" width="180" height="180" rx="40" ry="40" fill="url(#bgGradient)" filter="url(#shadow)"/>
  
  <!-- Inner Circle for Depth -->
  <circle cx="90" cy="90" r="65" fill="none" stroke="rgba(255,255,255,0.1)" stroke-width="2"/>
  
  <!-- Main Icon - Stylized 'EI' with Tech Elements -->
  <g fill="url(#iconGradient)">
    <!-- Letter 'E' -->
    <rect x="50" y="55" width="22" height="8" rx="4"/>
    <rect x="50" y="55" width="8" height="70" rx="4"/>
    <rect x="50" y="82" width="18" height="8" rx="4"/>
    <rect x="50" y="117" width="22" height="8" rx="4"/>
    
    <!-- Letter 'I' with Tech Accent -->
    <rect x="108" y="55" width="8" height="70" rx="4"/>
    <rect x="102" y="55" width="20" height="8" rx="4"/>
    <rect x="102" y="117" width="20" height="8" rx="4"/>
    
    <!-- Tech Circuit Elements -->
    <circle cx="85" cy="40" r="4" opacity="0.8"/>
    <circle cx="95" cy="40" r="3" opacity="0.6"/>
    <rect x="82" y="38" width="16" height="2" rx="1" opacity="0.7"/>
    
    <!-- Cloud Symbol (Top Right) -->
    <path d="M125 45c0-3 2.5-5.5 5.5-5.5s5.5 2.5 5.5 5.5c3 0 5.5 2.5 5.5 5.5s-2.5 5.5-5.5 5.5h-11c-3 0-5.5-2.5-5.5-5.5s2.5-5.5 5.5-5.5z" opacity="0.6"/>
    
    <!-- Security Shield (Bottom Left) -->
    <path d="M40 130l8-22 8 22c0 4.5-3.5 8-8 8s-8-3.5-8-8z" opacity="0.6"/>
    
    <!-- Network Nodes (Bottom Right) -->
    <circle cx="130" cy="130" r="3" opacity="0.7"/>
    <circle cx="140" cy="135" r="2.5" opacity="0.5"/>
    <circle cx="135" cy="140" r="2" opacity="0.4"/>
    <line x1="130" y1="130" x2="140" y2="135" stroke="url(#iconGradient)" stroke-width="1.5" opacity="0.5"/>
    <line x1="140" y1="135" x2="135" y2="140" stroke="url(#iconGradient)" stroke-width="1.5" opacity="0.5"/>
  </g>
  
  <!-- Subtle Highlight -->
  <rect x="0" y="0" width="180" height="180" rx="40" ry="40" fill="none" stroke="rgba(255,255,255,0.2)" stroke-width="2"/>
</svg>